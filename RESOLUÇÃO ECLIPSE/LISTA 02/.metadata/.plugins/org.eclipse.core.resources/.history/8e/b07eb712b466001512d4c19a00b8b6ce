package Fatec;

public class Exercicio_A {
	
	
		// inicia variaveis 
		public double x , y;
		// inicia default 
		Ponto(){
			this.x = 1.0;
			this.y = 1.0;
		}
		//recebe ordenada e abscissa
		Ponto(double x , double y){
			this.x = x;
			this.y = y;
		}
		// cria clone do ponto dado
		Ponto(Ponto P){
			this.x = P.x;
			this.y = P.y;
		}
		
		
		
		
	

}
